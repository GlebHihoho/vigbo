{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-return-assign": 0,
        "no-param-reassign": 0,
        "prefer-destructuring": 0,
        "react/prop-types": 0,
        "import/prefer-default-export": 0,
        "no-underscore-dangle": 0,
        "camelcase": 0
    }
}